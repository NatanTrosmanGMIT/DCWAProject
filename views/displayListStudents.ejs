<style>
    th {
      font-size: 14px;
      text-transform: uppercase;
      letter-spacing: 0.03em;
      box-shadow: 2px 2px 2px 2px rgba(0,0,0,0.1);
      border: 1px solid black;
      text-align: center;
    }
  
     td {
        padding: 10px 0;
        color: #2b2b2b;
          content: attr(data-label);
          flex-basis: 50%;
          text-align: right;
          border: 1px solid black;
          box-shadow: 2px 2px 2px 2px rgba(0,0,0,0.1);
          text-align: center;
      }
    </style>
  <table>
      <tr>
          <th>Student ID</th>
          <th>Name</th>
          <th>GPA</th>
          <th>Action</th>
      </tr>
      <% students.forEach((student)=> { %>
          <tr>
            <td><%= student.sid %> </td>
            <td><%= student.name %></td>
            <td><%= student.gpa %></td>
              <td><a href="<%= `/module/delete/students/${student.sid}` %>">Delete</a></td>
          </tr>
          <% }) %>
  </table>
  <br/>
  <button onclick="window.location.href='http://localhost:3000/addStudent%>'">Add Student</button><br/>
  <a href="/">Home</a>
  